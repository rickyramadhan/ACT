	                                  url: "process.php",
	                                  data: $("#contact_form").serialize(),
	                                  success: function(msg)
	                                  {
	                                      //alert(msg);
	                                      if(msg){
	                                      $('#contact_form').fadeOut(1000);
	                                      $('#contact_message').fadeIn(2000);
	                                      document.getElementById("contact_message").innerHTML = "Your email has been sent.";
	                                      return true;
	                                      }
	                                  }
	                              });
	                      } 
	                }

	          
